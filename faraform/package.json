{
  "name": "news.uk.fara",
  "version": "0.0.1",
  "description": "News UK Fara form",
  "scripts": {
    "clean": "gulp clean",
    "mydeploy": "gulp mydeploy",
    "serve": "ui5 serve --config=ui5.yaml",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build_old": "gulp build",
    "deploy_old": "run-s build mydeploy",
    "start": "fiori run --open \"test/flpSandbox.html?sap-ui-xx-viewCache=false#app-tile\"",
    "start-local": "fiori run --config ./ui5-local.yaml --open \"test/flpSandboxMockServer.html?sap-ui-xx-viewCache=false#app-tile\"",
    "start-noflp": "fiori run --open \"index.html?sap-ui-xx-viewCache=false\"",
    "start-variants-management": "fiori run --open \"preview.html?sap-ui-xx-viewCache=false&fiori-tools-rta-mode=true&sap-ui-rta-skip-flex-validation=true#preview-app\"",
    "build": "ui5 build -a --clean-dest --include-task=generateCachebusterInfo",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml && rimraf archive.zip",
    "deploy-config": "fiori add deploy-config",
    "start-mock": "fiori run --open \"test/flpSandboxMockServer.html?sap-ui-xx-viewCache=false#app-tile\"",
    "undeploy": "npm run build && fiori undeploy --config ui5-deploy.yaml",
    "deploy-test": "npm run build && fiori deploy --config ui5-deploy.yaml --testMode true"
  },
  "author": "shaun bradridge",
  "license": "ISC",
  "dependencies": {
    "gulp": "^5.0.0",
    "gulp-babel": "^8.0.0",
    "gulp-clean": "^0.4.0",
    "gulp-compass": "^2.1.0",
    "gulp-concat": "^2.6.1",
    "gulp-copy": "^5.0.0",
    "gulp-declare": "^0.3.0",
    "gulp-handlebars": "^5.0.2",
    "gulp-inline-image": "^0.0.2",
    "gulp-minify-css": "^1.2.3",
    "gulp-minify-html": "^0.1.0",
    "gulp-replace": "^1.1.3",
    "gulp-rev": "^9.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-ui5-preload": "^1.3.1",
    "gulp-usemin": "^0.3.30",
    "gulp-webserver": "^0.1.0",
    "gulp-wrap": "^0.15.0",
    "gulp-zip": "^5.1.0",
    "gulp4-run-sequence": "^1.0.1",
    "http-proxy-middleware": "^2.0.4",
    "lodash": "^4.17.21",
    "node-http-proxy": "^0.0.3",
    "q": "^1.5.1",
    "request": "^2.88.0",
    "walk": "^2.3.15",
    "xmlhttprequest": "^1.8.0",
    "yargs": "^17.4.0"
  },
  "devDependencies": {
    "@sap/ux-ui5-tooling": "^1.16.3",
    "@ui5/cli": "^3.0.0",
    "grunt": "1.6.1",
    "grunt-connect-proxy": "^0.1.5",
    "grunt-contrib-clean": "^2.0.0",
    "grunt-contrib-connect": "^3.0.0",
    "grunt-contrib-copy": "^1.0.0",
    "grunt-eslint": "^24.0.0",
    "grunt-openui5": "^0.18.4",
    "gulp-manifest3": "^0.1.2",
    "gulp-uglify": "^3.0.2",
    "mime": "^3.0.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "3.0.2",
    "ui5-middleware-livereload": "^0.5.9",
    "ui5-middleware-servestatic": "^0.3.9",
    "ui5-middleware-simpleproxy": "^0.8.0",
    "ui5-task-flatten-library": "^0.2.3",
    "ui5-task-i18ncheck": "^0.3.6",
    "ui5-task-nwabap-deployer": "^2.1.0",
    "ui5-task-stringreplacer": "^0.6.1",
    "ui5-task-transpile": "^0.3.6"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-livereload",
      "ui5-middleware-servestatic",
      "ui5-middleware-simpleproxy",
